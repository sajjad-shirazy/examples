  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/faker": "4.1.9",
    "@types/jest": "25.1.0",
    "@types/prompts": "2.0.3",
    "faker": "4.1.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "ts-jest": "25.0.0"
